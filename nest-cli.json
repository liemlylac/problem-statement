{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/problem-statement/src",
  "monorepo": true,
  "root": "apps/problem-statement",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/problem-statement/tsconfig.app.json"
  },
  "projects": {
    "problem-statement": {
      "type": "application",
      "root": "apps/problem-statement",
      "entryFile": "main",
      "sourceRoot": "apps/problem-statement/src",
      "compilerOptions": {
        "tsConfigPath": "apps/problem-statement/tsconfig.app.json"
      }
    },
    "queue": {
      "type": "application",
      "root": "apps/queue",
      "entryFile": "main",
      "sourceRoot": "apps/queue/src",
      "compilerOptions": {
        "tsConfigPath": "apps/queue/tsconfig.app.json"
      }
    }
  }
}