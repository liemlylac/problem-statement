{
  "name": "problem-statement",
  "version": "0.0.1",
  "description": "Problem statement using NestJs with RabbitMQ",
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/liemlylac/problem-statement/"
  },
  "author": "liemlylac",
  "license": "MIT",
  "scripts": {
    "doc": "compodoc -p apps/main/tsconfig.app.json -d docs/apps/main",
    "doc:queue": "compodoc -p apps/queue/tsconfig.app.json -d docs/apps/queue",
    "doc:libs": "compodoc -p libs/core/tsconfig.app.json -d docs/libs/core",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "prestart": "rimraf dist/apps/main",
    "start": "nest start",
    "prestart:queue": "rimraf dist/apps/queue",
    "start:queue": "nest start queue",
    "prestart:dev": "rimraf dist/apps/main",
    "start:dev": "nest start --watch",
    "prestart:queue:dev": "rimraf dist/apps/queue",
    "start:queue:dev": "nest start queue --watch",
    "start:debug": "nest start --debug --watch",
    "start:queue:debug": "nest start queue --debug --watch",
    "start:prod": "node dist/apps/problem-statement/main",
    "start:queue:prod": "node dist/apps/queue/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/main/test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.1.0",
    "@nestjs/common": "^9.0.1",
    "@nestjs/config": "^2.2.0",
    "@nestjs/core": "^9.0.1",
    "@nestjs/microservices": "^9.0.2",
    "@nestjs/platform-express": "^9.0.1",
    "@nestjs/swagger": "^6.0.1",
    "@nestjs/typeorm": "^9.0.0",
    "amqp-connection-manager": "^4.1.3",
    "amqplib": "^0.10.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "joi": "^17.6.0",
    "keycloak-connect": "^18.0.2",
    "mysql2": "^2.3.3",
    "nest-keycloak-connect": "^1.8.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@compodoc/compodoc": "^1.1.19",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/schematics": "^9.0.1",
    "@nestjs/testing": "^9.0.1",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/multer": "^1.4.7",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "28.1.2",
    "prettier": "^2.7.1",
    "source-map-support": "^0.5.21",
    "supertest": "^6.1.4",
    "ts-jest": "28.0.5",
    "ts-loader": "^9.3.1",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "4.0.0",
    "typescript": "^4.7.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": ".",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "verbose": true,
    "collectCoverage": false,
    "collectCoverageFrom": [
      "<rootDir>/apps/**/*.(ts|js)",
      "<rootDir>/libs/**/*.(ts|js)",
      "!<rootDir>/dist/**/*.(ts|js)",
      "!<rootDir>/**/*.(spec|e2e-spec|entity|interface|dto|module|mock).(ts|js)",
      "!<rootDir>/**/index.(ts|js)"
    ],
    "coverageDirectory": "<rootDir>/coverage",
    "coveragePathIgnorePatterns": [
      "/node_modules/",
      "package.json",
      "package-lock.json"
    ],
    "coverageReporters": [
      "lcov",
      "clover",
      "text-summary"
    ],
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/apps/",
      "<rootDir>/libs/"
    ],
    "moduleNameMapper": {
      "^@app/core(|/.*)$": "<rootDir>/libs/core/src/$1"
    }
  }
}
